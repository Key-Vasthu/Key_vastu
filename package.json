{
  "name": "keyvasthu",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:all": "concurrently \"npm run dev\" \"npm run server:dev\"",
    "dev:all:win": "start /B npm run dev & npm run server:dev",
    "build": "tsc -b && vite build",
    "start": "serve -s build -l 10000",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "node server/index.js",
    "server:dev": "node --watch server/index.js",
    "server:start": "node start-server.js",
    "start:backend": "node server/index.js",
    "test:server": "node test-server.js",
    "view-chat": "node view-chat-data.js",
    "upload-r2-images": "node upload-images-to-r2.js",
    "vercel-build": "npm run build",
    "preinstall": "npm config set audit false && npm config set fund false",
    "setup:api": "node -e \"const fs=require('fs');const env='.env';let content=fs.existsSync(env)?fs.readFileSync(env,'utf8'):'';if(!content.includes('VITE_API_URL')){content+='\\n# API Configuration\\nVITE_API_URL=http://localhost:3001/api\\n';fs.writeFileSync(env,content);console.log('✅ Added VITE_API_URL to .env');}else{console.log('ℹ️  VITE_API_URL already exists');}\"",
    "setup:api:ps1": "powershell -ExecutionPolicy Bypass -File setup-api-env.ps1",
    "setup:api:sh": "bash setup-api-env.sh"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.972.0",
    "@aws-sdk/s3-request-presigner": "^3.972.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fabric": "^5.5.2",
    "framer-motion": "^11.11.0",
    "lucide-react": "^0.460.0",
    "multer": "^2.0.2",
    "pg": "^8.11.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "serve": "^14.2.4",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fabric": "^5.3.0",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10"
  },
  "overrides": {
    "glob": "^10.0.0",
    "rimraf": "^5.0.0",
    "glob-parent": "^6.0.0",
    "whatwg-encoding": "^3.0.0",
    "w3c-hr-time": "^1.0.2",
    "domexception": "^4.0.0",
    "abab": "^2.0.6",
    "are-we-there-yet": "^4.0.0",
    "npmlog": "^7.0.0",
    "gauge": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=10.0.0"
  }
}
